			<name>playBeep</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:any digit, #, *. Default Value:1234567890*#</description>
			<name>finishOnKey</name>
			<required>true</required>
			<type>java.lang.String</type>
