		<tag-name>play</tag-name>
		<component>
			<component-type>play</component-type>
		</component>
		<attribute>
			<description>Allowed Values:integer &#60;&#61; 0. Default Value:1</description>
			<name>loop</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
