			<name>timeLimit</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true,false. Default Value:false</description>
			<name>hangupOnStar</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:no default action for Conference</description>
			<name>action</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:GET, POST. Default Value:POST</description>
			<name>method</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:none</description>
			<name>callbackUrl</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:GET, POST. Default Value:POST</description>
			<name>callbackMethod</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:List of digits patterns separated by comma. Default Value:none</description>
			<name>digitsMatch</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true,false. Default Value:false</description>
			<name>floorEvent</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true,false. Default Value:false</description>
			<name>redirect</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:auto - hybrid. Default Value:auto</description>
			<name>transcriptionType</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:none</description>
			<name>transcriptionUrl</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:GET, POST. Default Value:GET</description>
			<name>transcriptionMethod</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true, false. Default Value:true</description>
			<name>relayDTMF</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>The Dial element is used for dialing a single number or multiple numbers.</description>
		<tag-name>dial</tag-name>
		<component>
			<component-type>dial</component-type>
		</component>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:no default action for Dial</description>
			<name>action</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:GET, POST. Default Value:POST</description>
			<name>method</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true, false. Default Value:False</description>
			<name>hangupOnStar</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:positive integer (in seconds). Default Value:14400 seconds (4 hours)</description>
			<name>timeLimit</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:positive integer (seconds). Default Value:none</description>
			<name>timeout</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:valid phone number. Default Value: Current caller's callerId</description>
			<name>callerId</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:String or default. Default Value:Caller's callerName</description>
			<name>callerName</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:empty</description>
			<name>confirmSound</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:any digit, #, *. Default Value:empty</description>
			<name>confirmKey</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:empty</description>
			<name>dialMusic</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:empty</description>
			<name>callbackUrl</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:GET, POST. Default Value:POST</description>
			<name>callbackMethod</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true, false. Default Value:true</description>
			<name>redirect</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:List of digit patterns separated by comma. Default Value:empty</description>
			<name>digitsMatch</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:List of SIP headers to send, separated by commas. key=value. Default Value:None</description>
			<name>sipHeaders</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>The DTMF element is used to send digits from the current call.</description>
		<tag-name>dmtf</tag-name>
		<component>
			<component-type>dmtf</component-type>
		</component>
		<attribute>
			<description>Allowed Values:true,false. Default true</description>
			<name>async</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>The GetDigits element is used to collect the digits entered by a caller.</description>
		<tag-name>getDigits</tag-name>
		<component>
			<component-type>getDigits</component-type>
		</component>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:none</description>
			<name>action</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:GET, POST. Default Value:POST</description>
			<name>method</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:positive integer. Default Value:5 seconds</description>
			<name>timeout</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:positive integer. Default Value:2 seconds</description>
			<name>digitTimeout</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:any digit, #, *. Default Value:#</description>
			<name>finishOnKey</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:integer &#60;&#61; 1. Default Value:99</description>
			<name>numDigits</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:integer &#60;&#61; 1. Default Value:1</description>
			<name>retries</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true,false. Default Value:true</description>
			<name>redirect</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true,false. Default Value:false</description>
			<name>playBeep</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:any digit, #, *. Default Value:1234567890*#</description>
			<name>validDigits</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:Any remote sound file URL (mp3 or wav). Default Value:none</description>
			<name>invalidDigitsSound</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:true, false. Default Value:true</description>
			<name>log</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
