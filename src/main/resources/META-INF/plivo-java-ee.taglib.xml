	<tag>
		<description>The Hangup element is used to end a call.</description>
		<tag-name>hangup</tag-name>
		<component>
			<component-type>hangup</component-type>
		</component>
		<attribute>
			<description>Allowed Values:rejected, busy. Default Value:none</description>
			<name>reason</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:nteger > 0 (in seconds). Default Value:none</description>
			<name>schedule</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	<tag>
		<description>The Message element is used to send a message.</description>
		<tag-name>message</tag-name>
		<component>
			<component-type>message</component-type>
		</component>
		<attribute>
			<description>Allowed Values:Must be a purchased, valid number. Default Value:none</description>
			<name>src</name>
			<required>true</required>
