		<attribute>
			<description>Allowed Values:GET, POST. Default Value:GET</description>
			<name>transcriptionMethod</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:absolute URL. Default Value:none</description>
			<name>callbackUrl</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Allowed Values:GET, POST. Default Value:POST</description>
			<name>callbackMethod</name>
			<required>true</required>
			<type>java.lang.String</type>
